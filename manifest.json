{
    "name": "Real Time Constraints",
    "version": "1.2.3",
    "description": "Arebyte - Real Time Constraints",
    "permissions": ["storage", "alarms", "tabs", "idle"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "icons": { 
      "16": "icons/rtc-16.png",
      "24": "icons/rtc-24.png",
      "32": "icons/rtc-32.png",
      "48": "icons/rtc-48.png",
      "128": "icons/rtc-128.png"
    },
    "manifest_version": 2
}
